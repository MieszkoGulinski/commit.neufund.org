<div class="faq">
    <div class="row">
        <!--Nav Bar -->
        <nav class="col-md-3 bs-docs-sidebar hidden-sm hidden-xs">
            <ul id="sidebar" class="nav nav-stacked fixed">
            <% for ( var i = 0 ; i < faq.length ; i++ ) {
                var question = faq[i];
                %>
                <li><a class="smooth-scroll" href="#Group<%=i%>"><%=question.title%></a></li>
            <% } %> <!-- end title section questions for loop -->                  
            </ul>
        </nav>
        <!--Main Content -->
        <div id="faq-content" class="col-xs-12 col-md-9">
        <% for ( var i = 0 ; i < faq.length ; i++ ) {
            var question = faq[i];
            %>
            <section id="Group<%=i%>">
                <h4 class="title"><%=question.title%></h4>
                <% for ( var j = 0 ; j < question.questions.length ; j++ ) {
                    var subQuestion = question.questions[j];
                    %>                        
            <div class="group">
                <a href="#" class="show-answer"> <h4><%=subQuestion.question%> <i class="material-icons">keyboard_arrow_down</i></h4></a>
                <p class="hidden-p">
                    <%=subQuestion.answer%>
                </p>
            </div>                    
                    <% } %> <!-- end sub question for loop -->                  
            </section>
        <% } %> <!-- end title section questions for loop -->                  

        <!-- end main content -->
        </div> 
    </div>
</div>