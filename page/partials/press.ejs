<div class="section press">
    <div class="flex-row">
        <% for ( var i = 0 ; i < links.length ; i++ ) { %>
        <div class="img-container">
          <% if (links[i].link) { %>
          <a target="_blank" href="<%= links[i].link -%>" ><img src="<%= links[i].img -%>"/></a>
          <% } else { %>
          <img src="<%= links[i].img -%>"/>
          <% } %>
        </div>
        <% } %>
    </div>
</div>
